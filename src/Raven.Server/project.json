{
  "version": "4.0.0-*",
  "description": "Raven.Server is the database server for RavenDB",
  "authors": [ "Hibernating Rhinos" ],
  "tags": [ "database", "nosql", "doc db" ],
  "projectUrl": "https://ravendb.net",


  "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs" ],

  "compilationOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true
  },

  "dependencies": {
    "Raven.Client": "4.0.0-*",
    "Sparrow": "4.0.0-*",
    "Voron": "4.0.0-*"
  },

  "commands": {
    "Raven.Server": "Raven.Server"
  },
  
  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc3-23808",
        "System.Collections": "4.0.11-rc3-23808",
        "System.Console": "4.0.0-rc3-23808",
        "System.Linq": "4.0.1-rc3-23808",
        "System.Threading": "4.0.11-rc3-23808",
        "Microsoft.AspNet.Hosting": "1.0.0-rc2-16253",
        "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-16345",
        "Microsoft.Extensions.Configuration.FileProviderExtensions": "1.0.0-rc2-16035",
        "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-16035",
        "Microsoft.AspNetCore.WebSockets.Server": "0.1.0-rc2-16214"
      }
    }
  }
}
