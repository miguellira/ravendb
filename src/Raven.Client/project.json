{
    "version": "4.0.0-*",
    "description": "Raven.Client is the client library for accessing RavenDB",
    "authors": [
        "Hibernating Rhinos"
    ],
    "tags": [
        "database",
        "nosql",
        "doc db"
    ],
    "projectUrl": "https://ravendb.net",
    "dependencies": {
        "Sparrow": "4.0.0-*"
    },
    "compilationOptions": {
        "allowUnsafe": true
    },
    "compile": [
        "**/*.cs",
        "../CommonAssemblyInfo.cs",
        "../../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/**/*.cs"
    ],
    "compileExclude": [
        "../../Imports/Newtonsoft.Json/Src/Newtonsoft.Json/Properties/AssemblyInfo.cs"
    ],
    "frameworks": {
        "netcoreapp1.0": {
            "compilationOptions": {
                "define": [
                    "PORTABLE"
                ]
            },
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.0.0-*"
                },
                "Microsoft.Extensions.Configuration": "1.0.0-*",
                "System.Collections.Specialized": "4.0.1-*",
                "System.Net.WebSockets.Client": "4.0.0-*",
                "System.Security.Cryptography.Cng": "4.1.0-*"
            },
            "imports": [
                "dotnet5.4",
                "portable-net45+wp80+win8+wpa81+dnxcore50"
            ]
        }
    }
}